<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dllg.ebpay.dao.custom.AccountCentralCuMapper">


    <!--查询账户中心信息(个人)-->
    <select id="getAccountCentralInfo" resultType="com.dllg.ebpay.model.generator.User">
         select * from user where user_id = #{userId}
    </select>

    <!--查询账户中心信息(企业)-->
    <select id="getAccountCentralEnterpriseInfo" resultType="com.dllg.ebpay.model.generator.Company">
        select c.* FROM USER u LEFT JOIN company c ON c.company_id = u.user_company_id where u.user_id = #{userId}
    </select>


    <!--查询账户中心银行卡-->
    <select id="getAccountCentralCard" resultType="com.dllg.ebpay.model.generator.User">

    </select>

    <!--查询账户信息-->
    <select id="getAccountList" resultType="com.dllg.ebpay.model.generator.AccountSub">

        select * FROM account_sub where account_sub_relation_id = #{relationId}
    </select>




</mapper>